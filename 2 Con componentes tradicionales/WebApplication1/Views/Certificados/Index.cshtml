@model IEnumerable<Models.Certificados.RegistroDeEmision>

@{
    ViewBag.Title = "Emisiones realizadas";
}

<h2>Emisiones realizadas</h2>

<p>
    @Html.ActionLink("Emitir certificados", "Create")
</p>
<table class="table">
    <tr>
        <th>
            # Emisión
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Identificacion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Identificacion)
        </td>
        <td>
            @Html.ActionLink("Certificados emitidos", "Details", new { id=item.Identificacion })
        </td>
    </tr>
}
</table>